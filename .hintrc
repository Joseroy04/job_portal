{
  "extends": [
    "development"
  ],
  "hints": {
    "no-inline-styles": "off",
    "meta-viewport": "off",
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "axe/structure": "off",
    "meta-charset-utf-8": "off"
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_ff <= 110",
    "not firefox <= 111",
    "not and_chr <= 111",
    "not chrome <= 111",
    "not edge <= 111",
    "not ios_saf <= 16.3",
    "not opera <= 95",
    "not safari <= 16.3",
    "not samsung <= 20"
  ]
}